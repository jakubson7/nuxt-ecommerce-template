{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "error": {
      "type": "object",
      "properties": {
        "data": {
          "type": "object",
          "properties": {
            "emailNotFound": { "type": "string" },
            "incorrectEmailOrPassword": { "type": "string" }
          },
          "required": ["emailNotFound", "incorrectEmailOrPassword"]
        },
        "validation": {
          "type": "object",
          "properties": {}
        }
      },
      "required": ["data", "validation"]
    }
  },
  "required": ["error"]
}
